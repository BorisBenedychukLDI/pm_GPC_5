<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_2oh7ld"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_2oh7ld"
    tools:context=".MainActivity2oh7ld">

    <View
        android:id="@+id/v_2oh7ld"
        android:layout_width="135dp"
        android:layout_height="68dp"
        android:background="@drawable/custom_view_2oh7ld"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gl_2oh7ld" />

    <ImageView
        android:id="@+id/iv_logo_2oh7ld"
        android:layout_width="382dp"
        android:layout_height="240dp"
        android:contentDescription="@string/logo_2oh7ld"
        app:layout_constraintBottom_toTopOf="@+id/gl_2oh7ld"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_2oh7ld" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_2oh7ld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="418dp" />

    <TextView
        android:id="@+id/tv_start_2oh7ld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_2oh7ld"
        android:textColor="@color/theme_yellow_2oh7ld"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/v_2oh7ld"
        app:layout_constraintEnd_toEndOf="@+id/v_2oh7ld"
        app:layout_constraintStart_toStartOf="@+id/v_2oh7ld"
        app:layout_constraintTop_toTopOf="@+id/v_2oh7ld" />
</androidx.constraintlayout.widget.ConstraintLayout>